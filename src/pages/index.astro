---
import CapturedHtmlPage from "../components/CapturedHtmlPage.astro";
import { getCapturedPageBySourcePathname } from "../lib/captured-pages";

const homePage = getCapturedPageBySourcePathname("/");
if (!homePage) {
  throw new Error("Missing captured homepage source for '/'");
}
---

<CapturedHtmlPage rawHtml={homePage.rawHtml} />
